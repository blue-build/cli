{%- for module in modules_ext.modules %}

  {%- if let Some(type) = module.module_type %}
    {%- if type == "containerfile" %}
      {%- include "containerfile" -%}

    {%- else if type == "files" %}
      {%- include "file" -%}

    {%- endif %}
  {%- else if let Some(from_file) = module.from_file %}
    {{ self::get_module_from_file(from_file) }}
  {%- endif %}

{%- endfor %}

